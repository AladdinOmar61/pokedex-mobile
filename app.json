{
  "expo": {
    "name": "pokedex-mobile",
    "slug": "pokedex-mobile",
    "scheme": "myapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/pokeball.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/pokeball.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/pokeball.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.aladdino.pokedexmobile"
    },
    "web": {
      "favicon": "./assets/pokeball.png"
    },
    "plugins": [
      "expo-router",
      ["expo-font",
      {
      "fonts": [
        "./assets/Fonts/Silkscreen-Regular.ttf"
      ],
      "android": {
        "fonts": [
          {
            "fontFamily": "Silkscreen",
            "fontDefinitions": [
              {
                "path": "./assets/Fonts/Silkscreen-Bold.ttf",
                "weight": 700
              }
            ]
          }
        ]
      }
    }]
    ],
    "experiments": {
      "tsconfigPaths": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0bcb5744-da46-4293-a56b-546dce0f2e31"
      }
    }
  }
}
